{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "name": "organization"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "avatar": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "info": {
      "type": "json"
    },
    "comfirmed": {
      "type": "boolean"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user"
    },
    "rooms": {
      "collection": "room"
    }
  }
}
