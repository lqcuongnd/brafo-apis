{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "name": "report"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "detail": {
      "type": "text"
    },
    "priority": {
      "type": "integer"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "type": {
      "type": "string"
    },
    "medias": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "status": {
      "type": "string"
    },
    "room": {
      "model": "room",
      "via": "report"
    }
  }
}
